<form method="POST" action="/rentals/<%=@rental.id%>">
  <input id="hidden" type="hidden" name="_method" value="patch" />

  <label for="Owner">Owner's Name</label>
  <input type="text" id="owner" name="owner" value="<%=@rental.owner%>" />

  <label for="address">Address</label>
  <input type="text" id="address" name="address" value="<%=@rental.address%>" />

  <label for="bedrooms">Bedrooms</label>
  <input type="text" id="bedrooms" name="bedrooms" value="<%=@rental.bedrooms%>" />

  <label for="bathrooms">Bathrooms</label>
  <input type="text" id="bathrooms" name="bathrooms" value="<%=@rental.bathrooms%>" />

  <input type="submit" value="submit" />
</form>