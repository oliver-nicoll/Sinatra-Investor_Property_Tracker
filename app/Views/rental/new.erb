<form action="/rentals" method="POST">

  <label for="property_type">Property Type:</label>
  <input type="text" name="rental[property_type]"><br>

  <label for="address">Address</label>
  <input type="text" name="rental[address]"><br>

  <label for="bedrooms">Bedrooms</label>
  <input type="text" name="rental[bedrooms]"><br>

  <label for="bathrooms">Bathrooms</label>
  <input type="text" name="rental[bathrooms]"><br>

  <label for="lease_length">Lease Duration:</label>
  <input type="text" name="rental[lease_length]"><br>

  <label for="monthly_rent">Monthly Rent:</label>
  <input type="text" name="rental[monthly_rent]"><br>

  <label for="second_floor">Second Floor?</label>
  <input type="text" name="rental[second_floor]"><br>

  <label for="pets_allowed">Pet Allowed?</label>
  <input type="text" name="rental[pets_allowed]"><br>

  <label for="availability">Availability:</label>
  <input type="text" name="rental[availability]"><br>

  <input type="submit" value="Create New Rental">
</form>